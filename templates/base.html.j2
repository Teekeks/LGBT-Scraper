<!DOCTYPE html>
<html lang="en">
<head>
    {% block head %}
    <meta charset="UTF-8">
    <title>LGBT Reddit Scraper</title>
    <!-- Compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="/static/style.css">

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    {% endblock %}
</head>
<body>
  <nav class="teal lighten-1">
    <div class="nav-wrapper">
        <a href="#!" class="brand-logo">LGBT Reddit Scraper</a>
        <ul class="right">               
            <li>    
               <div class="center row">
                  <div class="col s12 " >
                    <div class="row" id="topbarsearch">
                      <div class="input-field col s6 s12">
                        <form>
                            <i class="material-icons prefix">search</i>
                            <input type="text" placeholder="Username" id="autocomplete-input" class="" name="u">
                        </form>
                        </div>
                      </div>
                    </div>
                  </div>          
              </li>                     
        </ul>
    </div>
  </nav>
  <div id="content" class="content">{% block content %}{% endblock %}</div>
    <!-- Compiled and minified JavaScript -->
  <div id="footer">{% block footer %}{% endblock %}</div>
</body>
</html>
